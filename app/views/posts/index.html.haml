
%section#about.about
	.container.panel.panel-default.text-center
		%h1 Blog Posts from Michelle Bonat
		.row
			.col-lg-12.text-center
				- @posts.each do |post|
					.post_wrapper
						%h2{:class => "title\""}= link_to post.title, post
						%p.date= post.created_at.strftime("%A, %d %b %Y %l:%M %p")